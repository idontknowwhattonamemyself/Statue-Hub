				   {
					   ["args"] = {
						   ["code"] = "Bqkz5gwaDv",
					   },
					   ["cmd"] = "INVITE_BROWSER",
					   ["nonce"] = "."
				   })
		   })
		
		else
			setclipboard("https://discord.gg/Bqkz5gwaDv")
